{
  "name": "@column-resizer/core",
  "version": "0.0.1",
  "license": "MIT",
  "repository": "github:Runjuu/react-simple-resizer",
  "bugs": "https://github.com/Runjuu/react-simple-resizer/issues",
  "homepage": "https://github.com/Runjuu/react-simple-resizer#readme",
  "author": "Runjuu",
  "keywords": [
    "resize",
    "resizer",
    "multi-column",
    "resize table",
    "resize columns",
    "resizable",
    "columns",
    "table",
    "range slider",
    "resize table columns"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "npm run clean && run-p build:cjs build:esm",
    "build:cjs": "tsc -p tsconfig.json -m CommonJS --outDir dist/cjs",
    "build:esm": "tsc -p tsconfig.json -m ESNext --outDir dist/esm",
    "clean": "rm -rf ./dist"
  },
  "dependencies": {
    "rxjs": "^7.5.7"
  }
}
